@using WEA.Core.Entities
@using WEA.SharedKernel.Resources
@using WEA.Presentation.Models
@model Organization
@{ 
    var data = ViewBag.SubHeaders as List<SubHeaderItemModel>;
    if (data == null)
    {
        data = new List<SubHeaderItemModel>();
    }
}


    <div class="subheader py-2 py-lg-6 subheader-solid" style="margin-bottom:25px;" id="kt_subheader">
        <div class="container-fluid d-flex align-items-center justify-content-between flex-wrap flex-sm-nowrap">
            <!--begin::Info-->
            <div class="d-flex align-items-center flex-wrap mr-1">
                <!--begin::Page Heading-->
                <div class="d-flex align-items-baseline flex-wrap mr-5">
                    <!--begin::Page Title-->
                    <h5 class="text-dark font-weight-bold my-1 mr-5">@Model.OrganizationName</h5>
                    <!--end::Page Title-->
                    <!--begin::Breadcrumb-->
                    <ul class="breadcrumb breadcrumb-transparent breadcrumb-dot font-weight-bold p-0 my-2 font-size-sm">
                        @foreach (var menu in data.OrderBy(m=>m.OrderIndex))
                        {
                            <li class="breadcrumb-item text-muted">
                                <a href="@menu.Url" class="text-muted">@menu.Text</a>
                            </li>
                        }
                    </ul>
                    <!--end::Breadcrumb-->
                </div>
                <!--end::Page Heading-->
            </div>
            <!--end::Toolbar-->
        </div>
    </div>